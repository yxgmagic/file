@layout("/common/_container.html"){
<div class="row">
    <div class="col-sm-12">
        <div class="ibox float-e-margins">
            <div class="ibox-title">
                <h5>数据服务注册管理</h5>
            </div>
            <div class="ibox-content">
                <div class="row row-lg">
                    <div class="col-sm-12">
                        <div class="row">
                            <div class="col-sm-3">
                                <#NameCon id="condition" name="名称" />
                            </div>
                            <div class="col-sm-3">
                                <#button name="搜索" icon="fa-search" clickFun="ServerAuth.search()"/>
                            </div>
                        </div>
                        <div class="hidden-xs" id="ServerAuthTableToolbar" role="group">
                            @if(shiro.hasPermission("/serverAuth/add")){
                                <#button name="添加" icon="fa-plus" clickFun="ServerAuth.openAddServerAuth()"/>
                            @}
                            @if(shiro.hasPermission("/serverAuth/update")){
                                <#button name="修改" icon="fa-edit" clickFun="ServerAuth.openServerAuthDetail()" space="true"/>
                            @}
                            @if(shiro.hasPermission("/serverAuth/delete")){
                                <#button name="删除" icon="fa-remove" clickFun="ServerAuth.delete()" space="true"/>
                            @}
                        </div>
                        <#table id="ServerAuthTable"/>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script src="${ctxPath}/static/modular/dms/serverAuth/serverAuth.js"></script>
@}
