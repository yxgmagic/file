@layout("/common/_container.html"){
<div class="row">
	<div class="col-sm-12">
		<div class="ibox float-e-margins">

			<div class="ibox-content">
				<div class="row row-lg">
					<div class="col-sm-12">
						<div class="row">
							<div class="col-sm-4">
								<#input id="begindt" name="开始时间"
										clickFun="laydate({istime: false, format: 'YYYY-MM-DD'})" />
							</div>
							<div class="col-sm-4">
								<#input id="enddt" name="结束时间"
										clickFun="laydate({istime: false, format: 'YYYY-MM-DD'})" />
							</div>
							<div class="col-sm-4">
								<div class="form-group">
									<label class="col-sm-3 control-label">报表类型</label>
									<div class="col-sm-9">
										<#SelectDict id="reportTemp" dictName="reportTemp" dataStyle="btn"
												cssName="form-control  "></#SelectDict>
									</div>
								</div>
							</div>
						</div>
						<div class="col-sm-12 text-center" style="margin-top: 20px">
							<#button name="搜索" icon="fa-search" clickFun="searchJasper()" />
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
<div>
	<iframe id="reportFrame" width="100%" height="1024"   src="" frameborder="no" border="0" marginwidth="0" marginheight="0" scrolling="no" allowtransparency="yes"></iframe>
</div>
<script  type="text/javascript">
    function  searchJasper() {

        console.log($("#pacode").val());
        var path='${ctxPath}/reportForm/jasperReport?filename='+$("#reportTemp").val()+'.jasper&begindt=' + $("#begindt").val()+'&enddt=' + $("#enddt").val();
        console.log(path);
        $("#reportFrame").attr("src", path);
        // document.frames("reportFrame").document.location=path;
    };

</script>






@}
