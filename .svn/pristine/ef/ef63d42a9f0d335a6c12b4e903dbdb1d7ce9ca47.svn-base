@layout("/common/_container.html"){
<div class="ibox float-e-margins">
    <div class="ibox-content">
        <div class="form-horizontal">

            <input type="hidden" id="id" value="">

            <div class="row">
                <div class="col-sm-6 b-r">
                           
                            <#input id="seqname" name="名称" underline="true"/>
                            <#input id="seqtype" name="编码序列类型" underline="true"/>
                            <#input id="seqlen" name="流水号长度" underline="true"/>
					<div class="form-group">
						<label class="col-sm-3 control-label">流水号是否连续</label>
						<div class="col-sm-9">
							<#SelectDict id="isseries" dictName="yes_no" dataStyle="btn"
								cssName="form-control  "></#SelectDict>
						</div>
					</div>


				</div>

                <div class="col-sm-6">
                          
                            <#input id="seqtab" name="取数表" underline="true"/>
                            <#input id="seqcol" name="取数列" underline="true"/>
                            <#input id="seqdept" name="站点/部门" underline="true"/>
                            <#input id="roadnum" name="车道"  underline="true"/>
                  
                </div>
            </div>

            <div class="row btn-group-m-t">
                <div class="col-sm-12 text-center">
                    <#button btnCss="info" name="保存" id="ensure" icon="fa-check" clickFun="SequenceInfoDlg.addSubmit()"/>
                    <#button btnCss="danger" name="取消" id="cancel" icon="fa-eraser" clickFun="SequenceInfoDlg.close()"/>
                </div>
            </div>
        </div>

    </div>
</div>
<script src="${ctxPath}/static/modular/system/sequence/sequence_info.js"></script>
@}
