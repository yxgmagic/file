@layout("/common/_container.html"){
<div class="ibox float-e-margins">
    <div class="ibox-content">
        <div class="form-horizontal">

            <input type="hidden" id="id" value="${item.id}">

            <div class="row">
                <div class="col-sm-6 b-r">
                           
                            <#input id="oefullid" name="oefullid" value="${item.oefullid}" underline="true"/>
                            <#input id="lawdocid" name="lawdocid" value="${item.lawdocid}" underline="true"/>
                            <#input id="lawdocjson" name="业务数据" value="${item.lawdocjson}" />
                </div>

                <div class="col-sm-6">
                            <#input id="inputjson" name="补充手打输入数据" value="${item.inputjson}" underline="true"/>
                            <#input id="procstatus" name="处理状态" value="${item.procstatus}" underline="true"/>
                            <#input id="ldtype" name="文书类型" value="${item.ldtype}" />
                </div>
            </div>

            <div class="row btn-group-m-t">
                <div class="col-sm-12 text-center">
                    <#button btnCss="info" name="保存" id="ensure" icon="fa-check" clickFun="OefullLawdocInfoDlg.editSubmit()"/>
                    <#button btnCss="danger" name="取消" id="cancel" icon="fa-eraser" clickFun="OefullLawdocInfoDlg.close()"/>
                </div>
            </div>
        </div>

    </div>
</div>
<script src="${ctxPath}/static/modular/oefulllawdoc/oefullLawdoc/oefullLawdoc_info.js"></script>
@}
