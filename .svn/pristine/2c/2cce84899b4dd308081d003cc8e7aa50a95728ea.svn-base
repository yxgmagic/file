@layout("/common/_container.html"){
<div class="ibox float-e-margins">
    <div class="ibox-content">
        <div class="form-horizontal">

            <input type="hidden" id="id" value="${item.id}">

            <div class="row">
                <div class="col-sm-6 b-r">
                            <#input id="id" name="" value="${item.id}" underline="true"/>
                            <#input id="pid" name="" value="${item.pid}" underline="true"/>
                            <#input id="areacode" name="行政区域代码" value="${item.areacode}" underline="true"/>
                            <#input id="areaname" name="行政区域名称" value="${item.areaname}" />
                </div>

                <div class="col-sm-6">
                            <#input id="arealetter" name="行政区域字母码" value="${item.arealetter}" underline="true"/>
                            <#input id="areatype" name="行政区类别" value="${item.areatype}" underline="true"/>
                            <#input id="address" name="地址" value="${item.address}" />
                </div>
            </div>

            <div class="row btn-group-m-t">
                <div class="col-sm-10">
                    <#button btnCss="info" name="提交" id="ensure" icon="fa-check" clickFun="AreaInfoDlg.editSubmit()"/>
                    <#button btnCss="danger" name="取消" id="cancel" icon="fa-eraser" clickFun="AreaInfoDlg.close()"/>
                </div>
            </div>
        </div>

    </div>
</div>
<script src="${ctxPath}/static/modular/platformConfig/area/area_info.js"></script>
@}
