<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhichao.dal.mapper.ChainListMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.zhichao.beans.guns.ChainList">
		<id column="id" property="id" />
		<result column="chainalias" property="chainalias" />
		<result column="chaindesc" property="chaindesc" />
		<result column="inchainclass" property="inchainclass" />
		<result column="outchainclass" property="outchainclass" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, chainalias, chaindesc, inchainclass, outchainclass
    </sql>

</mapper>
