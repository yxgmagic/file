@layout("/common/_container.html"){
<div class="ibox float-e-margins">
	<div class="ibox-content">
		<form id="areaForm">
			<div class="form-horizontal">

				<input type="hidden" id="id" value="">

				<div class="row">
					<div class="col-sm-6 b-r">
						<#input id="areacode" name="行政区域代码" value="" underline="true" />
						<#input id="areaname" name="行政区域名称" value=""  underline="true" />
						<#select id="pid" name="所属区域" value="" underline="false" />
					</div>

					<div class="col-sm-6">
						<#input id="arealetter" name="行政区域字母码" value="" underline="true" />
						<#input id="address" name="地址" value=""  underline="true" />
						<div class="col-sm-12">
							<div class="form-group">
								<label class="col-sm-3 control-label">行政区类别</label>
								<div class="col-sm-9">
									<#SelectDict id="areatype" name="areatype" dictName="areatype" dataStyle="btn"
										cssName="form-control"></#SelectDict>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>

			<div class="row btn-group-m-t text-center">
				<div class="col-sm-10">
					<#button btnCss="info" name="提交" id="ensure" icon="fa-check"
						clickFun="AreaInfoDlg.addSubmit()" />
					<#button btnCss="danger" name="取消" id="cancel" icon="fa-eraser"
						clickFun="AreaInfoDlg.close()" />

				</div>
			</div>
		</form>
	</div>
</div>
<script src="${ctxPath}/static/modular/platformConfig/administrativeregion/area_info.js"></script>
@}
