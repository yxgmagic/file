@layout("/common/_container.html"){
<div class="row">
    <div class="col-sm-12">
        <div class="ibox float-e-margins">
            <div class="ibox-title">
                <h5>抄告信息反馈管理</h5>
            </div>
            <div class="ibox-content">
                <div class="row row-lg">
                    <div class="col-sm-12">
                        <div class="row">
                            <div class="col-sm-3">
                                <#NameCon id="condition" name="名称" />
                            </div>
                            <div class="col-sm-3">
                                <#button name="搜索" icon="fa-search" clickFun="Reportsend.search()"/>
                            </div>
                        </div>
                        <div class="hidden-xs" id="ReportsendTableToolbar" role="group">
                            @if(shiro.hasPermission("/reportsend/add")){
                                <#button name="添加" icon="fa-plus" clickFun="Reportsend.openAddReportsend()"/>
                            @}
                            @if(shiro.hasPermission("/reportsend/update")){
                                <#button name="修改" icon="fa-edit" clickFun="Reportsend.openReportsendDetail()" space="true"/>
                            @}
                            @if(shiro.hasPermission("/reportsend/delete")){
                                <#button name="删除" icon="fa-remove" clickFun="Reportsend.delete()" space="true"/>
                            @}
                        </div>
                        <#table id="ReportsendTable"/>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script src="${ctxPath}/static/modular/report/reportsend/reportsend.js"></script>
@}
