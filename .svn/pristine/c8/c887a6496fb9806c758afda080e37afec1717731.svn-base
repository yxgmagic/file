@media (max-width:1360px)and (max-height:879px) {
    .container {
        width: 500px;
        height: 200px;
        top: -10%;
    }

    .wrap {
        padding: 50px;
    }

    .titlename {
        width: 700px;
        top: 14%;
    }
}

@media (min-width:1366px) and (max-height:879px) {
    .container {
        width: 500px;
        height: 200px;
        top: -10%;
    }

    .wrap {
        padding: 50px;
    }
}

@media (max-width:720px) {
    .container {
        width: 300px;
        height: 100px;
        top: -10%;
    }

    .login-input {
        height: 30px !important;
        font-size: 12px;
    }

    .titlename {
        width: 400px;
        top: 14%;
    }

    .wrap {
        padding: 20px;
    }

    .wrap button {
        font-size: 14px !important;
    }
}

