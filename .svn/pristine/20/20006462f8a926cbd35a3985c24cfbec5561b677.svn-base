@layout("/common/_container.html"){
<div class="row">
    <div class="col-sm-12">
        <div class="ibox float-e-margins">
            <div class="ibox-title">
                <h5>执法文书下载</h5>
            </div>
            <div class="ibox-content">
                <div class="row row-lg">
                    <div class="col-sm-12">
                        <div class="row">
                            <div class="col-sm-3">
                                <#NameCon id="condition" name="名称" />
                            </div>
                            <div class="col-sm-3">
                                <#button name="搜索" icon="fa-search" clickFun="LawDoc.search()"/>
                            </div>
                        </div>
                        <div class="hidden-xs" id="LawDocTableToolbar" role="group">
                            @if(shiro.hasPermission("/lawDoc/add")){
                                <#button name="添加" icon="fa-plus" clickFun="LawDoc.openAddLawDoc()"/>
                            @}
                            @if(shiro.hasPermission("/lawDoc/update")){
                                <#button name="修改" icon="fa-edit" clickFun="LawDoc.openLawDocDetail()" space="true"/>
                            @}
                            @if(shiro.hasPermission("/lawDoc/delete")){
                                <#button name="删除" icon="fa-remove" clickFun="LawDoc.delete()" space="true"/>
                            @}
                       <button class="btn btn-info"  name="查看模板"  id="ensure"  
						onclick="LawDoc.lookModelFile()" >  <i class="fa fa-check"></i>&nbsp;查看模板</button>
                        </div>
                        <#table id="LawDocTable"/>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script src="${ctxPath}/static/modular/lawdoc/lawDoc/lawDoc.js"></script>
@}
