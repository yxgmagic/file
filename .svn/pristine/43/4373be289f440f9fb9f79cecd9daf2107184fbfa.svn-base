@layout("/common/_container.html"){
<div class="row">
    <div class="col-sm-12">
        <div class="ibox float-e-margins">
            <div class="ibox-title">
                <h5>数据服务步骤参数管理</h5>
            </div>
            <div class="ibox-content">
                <div class="row row-lg">
                    <div class="col-sm-12">
                        <div class="row">
                            <div class="col-sm-3">
                                <#NameCon id="condition" name="名称" />
                            </div>
                            <div class="col-sm-3">
                                <#button name="搜索" icon="fa-search" clickFun="ActionPara.search()"/>
                            </div>
                        </div>
                        <div class="hidden-xs" id="ActionParaTableToolbar" role="group">
                            @if(shiro.hasPermission("/actionPara/add")){
                                <#button name="添加" icon="fa-plus" clickFun="ActionPara.openAddActionPara()"/>
                            @}
                            @if(shiro.hasPermission("/actionPara/update")){
                                <#button name="修改" icon="fa-edit" clickFun="ActionPara.openActionParaDetail()" space="true"/>
                            @}
                            @if(shiro.hasPermission("/actionPara/delete")){
                                <#button name="删除" icon="fa-remove" clickFun="ActionPara.delete()" space="true"/>
                            @}
                        </div>
                        <#table id="ActionParaTable"/>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script src="${ctxPath}/static/modular/dms/actionPara/actionPara.js"></script>
@}
