@layout("/common/_container.html"){
<div class="row">
    <div class="col-sm-12">
        <div class="ibox float-e-margins">
            <div class="ibox-title">
                <h5>数据服务链步骤关系维护管理</h5>
            </div>
            <div class="ibox-content">
                <div class="row row-lg">
                    <div class="col-sm-12">
                        <div class="row">
                            <div class="col-sm-3">
                                <#NameCon id="condition" name="名称" />
                            </div>
                            <div class="col-sm-3">
                                <#button name="搜索" icon="fa-search" clickFun="ChainAction.search()"/>
                            </div>
                        </div>
                        <div class="hidden-xs" id="ChainActionTableToolbar" role="group">
                            @if(shiro.hasPermission("/chainAction/add")){
                                <#button name="添加" icon="fa-plus" clickFun="ChainAction.openAddChainAction()"/>
                            @}
                            @if(shiro.hasPermission("/chainAction/update")){
                                <#button name="修改" icon="fa-edit" clickFun="ChainAction.openChainActionDetail()" space="true"/>
                            @}
                            @if(shiro.hasPermission("/chainAction/delete")){
                                <#button name="删除" icon="fa-remove" clickFun="ChainAction.delete()" space="true"/>
                            @}
                        </div>
                        <#table id="ChainActionTable"/>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script src="${ctxPath}/static/modular/dms/chainAction/chainAction.js"></script>
@}
