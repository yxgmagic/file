<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhichao.dal.mapper.ActionListMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.zhichao.beans.guns.ActionList">
		<id column="id" property="id" />
		<result column="actalias" property="actalias" />
		<result column="actdesc" property="actdesc" />
		<result column="actclass" property="actclass" />
		<result column="actmethod" property="actmethod" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, actalias, actdesc, actclass, actmethod
    </sql>

</mapper>
